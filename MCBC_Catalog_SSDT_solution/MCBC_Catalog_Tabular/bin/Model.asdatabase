          {
            "type": "calculated",
            "name": "Length of Relationship",
            "dataType": "string",
            "isDataTypeInferred": true,
            "expression": [
              "",
              "var diff = ",
              "    DATEDIFF(",
              "        DATE(",
              "            LEFT(v_dim_customer[Customer_Since],4),",
              "            MID(v_dim_customer[Customer_Since],5,2),",
              "            RIGHT(v_dim_customer[Customer_Since],2)",
              "        ),",
              "        TODAY()",
              "        ,DAY",
              "    )",
              "return",
              "    SWITCH(",
              "        TRUE(),",
              "        diff < 365, \"< 1Y\",",
              "        diff < 730, \"1 - 2Y\",",
              "        diff < 1825, \"3 - 5Y\",",
              "         \"5Y +\"",
              "    )",
              "",
              "",
              ""
            ]
          },
