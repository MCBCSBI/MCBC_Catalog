          },
          {
            "type": "calculated",
            "name": "Age Group",
            "dataType": "string",
            "isDataTypeInferred": true,
            "expression": [
              "",
              "var age = ",
              "DATEDIFF(",
              "    v_dim_customer[Date_Of_Birth],",
              "    TODAY(),",
              "    YEAR",
              ")",
              "return",
              "SWITCH(",
              "    TRUE(),",
              "    age < 18, \"< 18\",",
              "    age < 26, \"18 -25\",",
              "    age < 36, \"26 -35\",",
              "    age < 56, \"36 -55\",",
              "    \"55 +\"",
              ")"
            ]
          },
